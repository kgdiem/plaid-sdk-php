{
    "name": "kgdiem/plaid-sdk-php",
    "description": "PHP implementation of Plaid's API as an SDK.",
    "type": "library",
    "require": {
        "php": "~7.2",
        "ext-json": "*",
        "ext-curl": "*",
        "nimbly/shuttle": "^0.2.0"
    },
    "autoload": {
        "psr-4": {
            "kgdiem\\Plaid\\" : "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1",
        "vimeo/psalm": "^3.3",
        "symfony/var-dumper": "^4.2"
    },
    "autoload-dev": {
        "psr-4": {
            "kgdiem\\Plaid\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Brent Scheffler",
            "email": "brent@tomorrow.me"
        },
        {
            "name": "Kevin Diem",
            "email": "kevin@kevindiem.com"
        }
    ]
}
